class FieldValue(dict):
    def __init__(self, definition, value):
        self.definition = definition
        self['value'] = value

    def insert(self):
        self['definitionId'] = self.definition.new_id
    
    def __str__(self):
        return f"Field [{self.definition.id} -> {self.definition.new_id}] ({self['value']})"
